{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Observability": {
        "EnableOtelWcfInstrumentation": false,
        "EnableOtelSqlInstrumentation": true,
        "EnableOtelHttpInstrumentation": true,
        "EnableOtelAspNetInstrumentation": true,
        "DisableRemoteTracing": false,
        "IsMetricsEnabled": true
    }
}
